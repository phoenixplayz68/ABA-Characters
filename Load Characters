input = game:GetService("UserInputService").InputBegan:Connect(function(key,chat)
	if chat then return end
	if key.KeyCode.Name == "M" then
		if _G.ABACons then
			for i,v in pairs(_G.ABACons) do
				v:Disconnect()
			end
		end
	end
end)
