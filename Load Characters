--Base Stuff
if _G.input then _G.input:Disconnect() _G.input=nil end
if _G.teleporter then _G.teleporter:Disconnect() _G.teleporter=nil end

local TeleportCheck = false
game.Players.LocalPlayer.OnTeleport:Connect(function(State)
	if not TeleportCheck then
		TeleportCheck = true
		queue_on_teleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/phoenixplayz68/ABA-Characters/refs/heads/main/Load%20Characters'))()")
	end
end)

_G.input = game:GetService("UserInputService").InputBegan:Connect(function(key,chat)
	if chat or _G.Akaza==nil or _G.ABACons==nil then return end
	if key.KeyCode.Name == "M" then
		_G.input:Disconnect() _G.input=nil
		if _G.ABACons then
			for i,v in pairs(_G.ABACons) do
				v:Disconnect()
			end
		end
	end
end)
loadstring(game:HttpGet('https://raw.githubusercontent.com/phoenixplayz68/ABA-Characters/refs/heads/main/Basic%20Functions'))()
repeat task.wait() until _G.ABACons~=nil

local oldAkaza = _G.Akaza

--Characters
loadstring(game:HttpGet('https://raw.githubusercontent.com/phoenixplayz68/ABA-Characters/refs/heads/main/Yoriichi'))()
loadstring(game:HttpGet('https://raw.githubusercontent.com/phoenixplayz68/ABA-Characters/refs/heads/main/Muzan'))()
loadstring(game:HttpGet('https://raw.githubusercontent.com/phoenixplayz68/ABA-Characters/refs/heads/main/Gold%20Roger'))()
loadstring(game:HttpGet('https://raw.githubusercontent.com/phoenixplayz68/ABA-Characters/refs/heads/main/Akaza'))()
----
